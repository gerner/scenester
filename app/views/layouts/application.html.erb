<div id="tagline" style="font-size:18pt;font-weight:bold">Filtering Seattle <span>music, tech, art, film, theater</span> events so you don&#039;t have to.</div>
<script>
var taglineParts = [];
function initTagline() {
taglineParts = $("#tagline span").text().split(",");
$("#tagline span").css("width", "3.5em").css("display", "inline-block");
animateTagline();
}
function animateTagline() {
$("#tagline span").html("<span>"+taglineParts[Math.floor(Math.random()*taglineParts.length)]+"</span>");
$("#tagline span span").typewriter(1200, function() { jQuery("#tagline span span").fadeOut(1000, function() { animateTagline(); });});
}
$(document).ready(initTagline);
</script>

